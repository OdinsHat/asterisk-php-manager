<refentry id="package.net.net-asteriskmanager.asteriskmanager.--construct">
   <refnamediv>
    <refname>constructor <function>AsteriskManager::__construct</function></refname>
    <refpurpose>Class constructor</refpurpose>
   </refnamediv>
   <refsynopsisdiv>
    <funcsynopsis>
     <funcsynopsisinfo>
      require_once &apos;/asterisk.php&apos;;
     </funcsynopsisinfo>
     <funcprototype>
      <funcdef>void      constructor <function>AsteriskManager::__construct</function></funcdef>
      <paramdef>string <parameter>$server</parameter></paramdef>
      <paramdef>string <parameter>$username</parameter></paramdef>
      <paramdef>string <parameter>$password</parameter></paramdef>
      <paramdef>integer <parameter> <optional>$port = 5038</optional></parameter></paramdef>
     </funcprototype>
    </funcsynopsis>
    </refsynopsisdiv>

    <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.--construct.desc">
     &title.desc;
     &notdocumented;
    </refsect1>
   <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.--construct.param">
    &title.param;
    <para>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.string;
                         <parameter>$server</parameter>
       </term>
       <listitem>
        <para>
         The server hostname or IP address
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.string;
                         <parameter>$username</parameter>
       </term>
       <listitem>
        <para>
         Username credential for the manager interface
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.string;
                         <parameter>$password</parameter>
       </term>
       <listitem>
        <para>
         The password for the manager interface
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.int;
                         <parameter>$port</parameter>
       </term>
       <listitem>
        <para>
         The port the interface is listening on
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
    </para>
   </refsect1>
   <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.--construct.throws">
    &title.throws;
        <para>
      <emphasis>throws</emphasis> no exceptions thrown
    </para>
       </refsect1>
   <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.--construct.see">
    &title.see;
        <para>
      <emphasis>see</emphasis> AsteriskManager::$_socket
    </para>
        <para>
      <emphasis>see</emphasis> <varname>AsteriskManager::$username</varname>
    </para>
        <para>
      <emphasis>see</emphasis> <varname>AsteriskManager::$server</varname>
    </para>
        <para>
      <emphasis>see</emphasis> <varname>AsteriskManager::$port</varname>
    </para>
        <para>
      <emphasis>see</emphasis> <varname>AsteriskManager::$password</varname>
    </para>
       </refsect1>
   <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.--construct.note">
    &title.note;
    &note.notstatic;
   </refsect1>
</refentry>
