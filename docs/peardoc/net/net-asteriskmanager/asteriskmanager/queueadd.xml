<refentry id="package.net.net-asteriskmanager.asteriskmanager.queueadd">
   <refnamediv>
    <refname><function>AsteriskManager::queueAdd</function></refname>
    <refpurpose>Add a handset to a queue on the server</refpurpose>
   </refnamediv>
   <refsynopsisdiv>
    <funcsynopsis>
     <funcsynopsisinfo>
      require_once &apos;/asterisk.php&apos;;
     </funcsynopsisinfo>
     <funcprototype>
      <funcdef>bool      <function>AsteriskManager::queueAdd</function></funcdef>
      <paramdef>string <parameter>$queue</parameter></paramdef>
      <paramdef>string <parameter>$handset</parameter></paramdef>
      <paramdef>integer <parameter>$penalty</parameter></paramdef>
     </funcprototype>
    </funcsynopsis>
    </refsynopsisdiv>

    <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.queueadd.desc">
     &title.desc;
     &notdocumented;
    </refsect1>
   <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.queueadd.param">
    &title.param;
    <para>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.string;
                         <parameter>$queue</parameter>
       </term>
       <listitem>
        <para>
         The name of the queue you wish to add the handset too
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.string;
                         <parameter>$handset</parameter>
       </term>
       <listitem>
        <para>
         The handset to add, e.g. SIP/234
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.int;
                         <parameter>$penalty</parameter>
       </term>
       <listitem>
        <para>
         Penalty
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
    </para>
   </refsect1>
   <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.queueadd.throws">
    &title.throws;
        <para>
      <emphasis>throws</emphasis> no exceptions thrown
    </para>
       </refsect1>
   <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.queueadd.note">
    &title.note;
    &note.notstatic;
   </refsect1>
</refentry>
